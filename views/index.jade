!!! 5
html
  head
    title twitTrip
    meta(name="viewport", content="initial-scale=1.0, user-scalable=no")
    meta(charset="utf-8")
    style
      html, body {
        height: 100%;
        margin: 0;
        padding: 0;
        font: 12px/1.0 helvetica,sans-serif;
      }
      #map_canvas {
        margin-top: -50%;
        margin-left: -50%;
        height: 200%;
        width: 200%;
        box-shadow: inside 0 0 6px 6px rgba(255,255,255,0.72)
        -webkit-transform-origin: 50% 50%;
      }
      #string {
        position: fixed;
        top: 0;
        left: 0;
        right: 0;
        height: 50px;
        line-height: 50px;
        z-index: 99999;
      }
      @media print {
        html, body {
          height: auto;
        }
        #map_canvas {
          height: 650px;
        }
      }
    script(src="https://maps.googleapis.com/maps/api/js?sensor=true")
    script(src='http://maps.google.com/maps/api/js?sensor=true')
    script(src='/javascripts/jquery-1.8.0.min.js')
    script(src='/javascripts/script.js')
    script

  body
    div#map_canvas
      div#direction
    div#string
    dic#slider
